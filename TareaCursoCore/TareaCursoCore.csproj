    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>5efd81b5-277a-4dac-bf8a-424626a7c648</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore" Version="8.0.10" />
